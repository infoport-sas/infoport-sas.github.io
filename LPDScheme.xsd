<?xml version="1.0" encoding="UTF-8"?>
<!--LPDScheme.xsd V1.1-->

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <!--    Sixth level complex types-->
    <xsd:simpleType name="FullLessType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FCL"/>
            <xsd:enumeration value="LCL"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="FCLLCLType">
        <xsd:sequence>
            <xsd:element type="FullLessType" name="DepartureFCLLCL"/>
            <xsd:element type="FullLessType" name="ArrivalFCLLCL"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MeasureType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:float">
                <xsd:attribute name="unitCode">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="kg"/>
                            <xsd:enumeration value="m"/>
                            <xsd:enumeration value="KG"/>
                            <xsd:enumeration value="M"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="DimensionsType">
        <xsd:sequence>
            <xsd:element type="MeasureType" name="HeightMeasure" minOccurs="0"/>
            <xsd:element type="MeasureType" name="LengthMeasure" minOccurs="0"/>
            <xsd:element type="MeasureType" name="WidthMeasure" minOccurs="0"/>
            <xsd:element type="MeasureType" name="SquareMeasure" minOccurs="0"/>
            <xsd:element type="MeasureType" name="CubeMeasure" minOccurs="0"/>
            <xsd:element type="MeasureType" name="WeightMeasure"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SealType">
        <xsd:all>
            <xsd:element type="xsd:string" name="SealCode"/>
            <xsd:element name="SealType" minOccurs="0">
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>
                            CA Agent (Agent)
                            CU Customs (Douanes)
                            SA Heath controler (Sanitaire)
                            SU Supplier (Fournisseur)
                            TO Stevedor (Acconier)
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="CA"/>
                        <xsd:enumeration value="CU"/>
                        <xsd:enumeration value="SA"/>
                        <xsd:enumeration value="SU"/>
                        <xsd:enumeration value="TO"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="TemperatureType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:float">
                <xsd:attribute name="unitCode">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="C"/>
                            <xsd:enumeration value="F"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="TemperatureDescriptionType">
        <xsd:all>
            <xsd:element type="TemperatureType" name="TemperatureMinimum"/>
            <xsd:element type="TemperatureType" name="TemperatureMaximum"/>
            <xsd:element type="TemperatureType" name="TemperatureAverage"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="LoadingTerminalType">
        <xsd:sequence>
            <xsd:element type="xsd:dateTime" name="ReceivedDateTime"/>
            <xsd:element type="xsd:date" name="LoadDate"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    Fifth level complex types-->
    <xsd:complexType name="HazardousDescriptionType">
        <xsd:sequence>
            <xsd:element name="UNCode">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="^[0-9]{4}$"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ClassCode">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="1.1"/>
                        <xsd:enumeration value="1.2"/>
                        <xsd:enumeration value="1.3"/>
                        <xsd:enumeration value="1.4"/>
                        <xsd:enumeration value="1.5"/>
                        <xsd:enumeration value="1.6"/>
                        <xsd:enumeration value="2.1"/>
                        <xsd:enumeration value="2.2"/>
                        <xsd:enumeration value="2.3"/>
                        <xsd:enumeration value="3"/>
                        <xsd:enumeration value="4.1"/>
                        <xsd:enumeration value="4.2"/>
                        <xsd:enumeration value="4.3"/>
                        <xsd:enumeration value="5.1"/>
                        <xsd:enumeration value="5.2"/>
                        <xsd:enumeration value="6.1"/>
                        <xsd:enumeration value="6.2"/>
                        <xsd:enumeration value="7"/>
                        <xsd:enumeration value="8"/>
                        <xsd:enumeration value="9"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="WrappingGroup">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="I"/>
                        <xsd:enumeration value="II"/>
                        <xsd:enumeration value="III"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CargoUnitType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="IdentificationID">
                <xsd:annotation>
                    <xsd:documentation>
                        Cargo Unit ID (mandatory)
                        if CargoStateCode in Consignment is:
                        "CONT" : Container number,
                        "VEHI" : VIN,
                        "CONV" : an unique ID number
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TCType" minOccurs="0">
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>
                            Mandatory only if CargoStateCode in Consignment is "CONT"
                            Any ISOCode for container (as 22G0, 22R1, 42G0, 45R0,...)
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="^[1234BCGHLMN][0245689CDEF][A-Z0-9]{2}$"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element type="xsd:boolean" name="Empty" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Mandatory only if CargoStateCode in Consignment is "CONT"
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element type="xsd:positiveInteger" name="NumberPackages"/>
            <xsd:element type="xsd:string" name="Comment" minOccurs="0"/>
            <xsd:element type="FCLLCLType" name="FCLLCL"/>
            <xsd:element type="DimensionsType" name="Dimensions"/>
            <xsd:element type="SealType" name="Seal" maxOccurs="unbounded" minOccurs="0"/>
            <xsd:element type="TemperatureDescriptionType" name="TemperatureDescription" minOccurs="0"/>
            <xsd:element type="LoadingTerminalType" name="LoadingTerminal" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

