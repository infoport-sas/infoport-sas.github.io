    <!--    CodeType and PortsType -->
    <xsd:simpleType name="CodeType">
        <xsd:restriction base="xsd:string">
            <xsd:length value="5"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="PortsType">
        <xsd:sequence>
            <xsd:element type="CodeType" name="OriginPortCode"/>
            <xsd:element type="xsd:string" name="FromName"/>
            <xsd:element type="CodeType" name="LoadPortCode"/>
            <xsd:element type="xsd:string" name="LoadPortName"/>
            <xsd:element type="CodeType" name="DischargePortCode"/>
            <xsd:element type="xsd:string" name="DischargePortName"/>
            <xsd:element type="CodeType" name="DestinationPortCode"/>
            <xsd:element type="xsd:string" name="DestinationPortName"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    PartyType -->
    <xsd:complexType name="AddressType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="Line1"/>
            <xsd:element type="xsd:string" name="Line2" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="CountryCodeType">
        <xsd:restriction base="xsd:string">
            <xsd:length value="2"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="PartyType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="Code"/>
            <xsd:element type="xsd:string" name="Name"/>
            <xsd:element type="AddressType" name="Address" minOccurs="0"/>
            <xsd:element type="xsd:string" name="CityName" minOccurs="0"/>
            <xsd:element type="CountryCodeType" name="CountryCode" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    Third level complex types-->
    <xsd:complexType name="VesselType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="Code"/>
            <xsd:element type="xsd:string" name="Name"/>
            <xsd:element type="xsd:string" name="VoyageNumeric"/>
            <xsd:element type="xsd:string" name="FlagCode" minOccurs="0"/>
            <xsd:element type="xsd:string" name="LloydsID" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DischargePortType">
        <xsd:sequence>
            <xsd:element type="CodeType" name="PortCode" fixed="REPDG"/>
            <xsd:element type="xsd:string" name="PortName"/>
            <xsd:element type="xsd:dateTime" name="EstimatedArrivalDate"/>
            <xsd:element type="xsd:dateTime" name="EstimatedDepartureDate"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BillOfLadingType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="IdentificationID">
                <xsd:annotation>
                    <xsd:documentation>
                        BL number (mandatory)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element type="xsd:string" name="IssueDate"/>
            <xsd:element type="PartyType" name="Shipper"/>
            <xsd:element type="PartyType" name="Consignee"/>
            <xsd:element type="PartyType" name="Notify"/>
            <xsd:element type="xsd:string" name="MRN">
                <xsd:annotation>
                    <xsd:documentation>
                        MRN (reference number) of Customs ICS ENS Movement (mandatory for non EU goods)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element type="PortsType" name="Ports"/>
            <xsd:element type="ConsignmentType" name="Consignment" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    Second level complex types-->
    <xsd:complexType name="MfsType">
        <xsd:sequence>
            <xsd:element type="PartyType" name="Carrier"/>
            <xsd:element type="VesselType" name="Vessel"/>
            <xsd:element type="DischargePortType" name="DischargePort"/>
            <xsd:element type="BillOfLadingType" name="BillOfLading" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    First level complex types-->
    <xsd:complexType name="MessageHeaderType">
        <xsd:sequence>
            <xsd:element type="xsd:string" name="XDIJobId">
                <xsd:annotation>
                    <xsd:documentation>
                        This is a unique reference for the whole message
                        One message, one manifest.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ManifestType">
        <xsd:sequence>
            <xsd:element name="MessageHeader" type="MessageHeaderType"/>
            <xsd:element type="MfsType" name="Mfs"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--    Root message-->
    <xsd:element name="Manifest" type="ManifestType"/>

</xsd:schema>